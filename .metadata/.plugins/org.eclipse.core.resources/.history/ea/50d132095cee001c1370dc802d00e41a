package schoolGradeSys;

public class MeuEstudante {
		String name;
		String classe;
		double mediaAluno;
		MeuEstudante(){
			dia=1;
			mes=1;
			ano=1970;
			
		}
		MeuEstudante(String diaInicial,String mesInicial,int anoInicial) {
			dia=diaInicial;
			mes=mesInicial;
			ano=anoInicial;
		}
		
}
