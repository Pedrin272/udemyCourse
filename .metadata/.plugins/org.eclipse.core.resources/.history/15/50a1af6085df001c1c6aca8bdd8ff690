		System.out.println("Insira o salario do mes passado: ");
		String mes1 = sc.next();
		System.out.println("Insira o salario do mes anterior: ");
		String mes2 = sc.next();
		System.out.println("Insira o salario do mes anterior: ");
	    String mes3 = sc.next();
	    
	    Double priemiro = (Double) mes1;
	}

}
